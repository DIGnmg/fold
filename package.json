{
    "name": "fold",
    "displayName": "Fold",
    "description": "New way of thinking about panes.",
    "version": "0.0.1",
    "publisher": "DIGnmg",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.fold"
    ],
    "main": "./extension",
    "contributes": {
